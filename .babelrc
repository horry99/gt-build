{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ], // 装饰器
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ], // class语法
        "@babel/plugin-transform-runtime" // generator,promise等更高级的语法
    ]
}